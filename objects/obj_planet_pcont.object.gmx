        avevx=0
        avevy=0
        for (i=global.infoend; i &lt; ds_list_size(pinfo); i++){
            blockinst = pinfo[|i]
            avevx += blockinst.pseudo_hspd * blockinst.mass
            avevy += blockinst.pseudo_vspd * blockinst.mass
        }
        
        myvx = avevx/mymass
        myvy = avevy/mymass
    }
    // NEED TO ADD TERMINAL VELOCITY
    
    pinfo[|3] = myvx
    pinfo[|4] = myvy
    
    pinfo[|1] += myvx
    pinfo[|2] += myvy
    for (i=global.infoend; i &lt; ds_list_size(pinfo); i++){
        blockinst = pinfo[|i]
        blockinst.x += myvx
        blockinst.y += myvy
    }
}

